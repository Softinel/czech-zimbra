#. # The following errors happen when trying to join a chat room
#: im.Banned
msgid "You have been banned from the room."
msgstr ""

#: im.MaxUsers
msgid "The room has reached capacity."
msgstr ""

#: im.MustUseReservedRoomnick
msgid "You must use a reserved nickname."
msgstr ""

#: im.NicknameConflict
msgid "Someone else is already using your nickname."
msgstr ""

#: im.NoSuchRoom
msgid "No such room."
msgstr ""

#: im.NotAllowed
msgid "You are not allowed to create the room."
msgstr ""

#: im.NotAMember
msgid "The room is invite-only, and you aren't a member."
msgstr ""

#: im.PasswordRequired
msgid "A password is required to enter the room."
msgstr ""

#: im.Unknown
msgid "Unknown error."
msgstr ""

#: im.INVALID_ADDRESS
msgctxt "im.INVALID_ADDRESS"
msgid "The address is invalid."
msgstr ""

#: mail.ALREADY_EXISTS
msgid "An object with that name already exists."
msgstr "Objekt s takovým názvem už existuje."

#: mail.MODIFY_CONFLICT
msgid ""
"Item version number conflict. Another version was added between edit and "
"save."
msgstr ""
"Konflikt čísel verzí položek. Mezi započetím úprav a uložením byla "
"realizovaná další verze. "

#: mail.INVALID_NAME
msgid ""
"Sorry, \"{0}\" is not a valid name. It contains at least one invalid "
"character."
msgstr "Promiňte, „{0}“ není platné jméno. Obsahuje aspoň jeden neplatný znak."

#: mail.INVITE_OUT_OF_DATE
msgid "The invite has been modified and is no longer current."
msgstr ""

#: mail.MAINTENANCE
msgctxt "mail.MAINTENANCE"
msgid "This account is currently in maintenance mode."
msgstr ""

#: mail.MESSAGE_TOO_BIG
msgid ""
"This message cannot be sent because it has exceeded the maximum allowed "
"size."
msgstr ""
"Tuto zprávu není možné odeslat, protože překročila maximální povolenou "
"velikost."

#: mail.NO_SUCH_CONV
msgid "No such conversation exists."
msgstr "Taková konverzce neexistuje."

#: mail.NO_SUCH_FOLDER
msgid "No such folder exists."
msgstr "Takový adresář neexistuje."

#: mail.NO_SUCH_ITEM
msgid "Item \"{0}\" does not exist."
msgstr "Položka „{0}“ neexistuje."

#: mail.NO_SUCH_CALITEM
msgid ""
"Calendar or task item \"{0}\" does not exist. It may have been rescheduled."
msgstr ""

#: mail.NO_SUCH_MSG
msgid "No such message exists."
msgstr "Taková zpráva neexistuje."

#: mail.NO_SUCH_PART
msgid "No such message part exists."
msgstr "Taková část zprávy neexistuje."

#: mail.NO_SUCH_TAG
msgctxt "mail.NO_SUCH_TAG"
msgid "No such tag exists."
msgstr "Takový štítek neexistuje."

#: mail.QUERY_PARSE_ERROR
msgid "Unable to parse the search query."
msgstr "Nebylo možné analyzovat vyhledávací řetězec."

#: mail.QUOTA_EXCEEDED
msgid ""
"Your message could not be sent because you have exceeded your mail quota."
msgstr ""
"Vaši zprávu nebylo možné odeslat, protože jste překročili svoji kvótu pro "
"poštu. "

#: mail.TOO_MANY_CONTACTS
msgid ""
"Contact could not be created because you have exceeded your contact quota."
msgstr ""
"Kontakt nebylo možné vytvořit, protože jste překročili svoji kvótu pro "
"kontakty."

#: mail.TOO_MANY_QUERY_TERMS_EXPANDED
msgid "Please be more specific in your search criteria."
msgstr "Prosím, upřesněte své vyhledávací požadavky."

#: mail.MAIL_UNABLE_TO_IMPORT_APPOINTMENTS
msgctxt "mail.MAIL_UNABLE_TO_IMPORT_APPOINTMENTS"
msgid "Unable to import. The chosen file may be invalid."
msgstr "Nebylo možné importovat. Zvolený soubor je zřejmě neplatný."

#: mail.MAIL_UNABLE_TO_IMPORT_CONTACTS
msgctxt "mail.MAIL_UNABLE_TO_IMPORT_CONTACTS"
msgid "Unable to import. The chosen file may be invalid."
msgstr "Nebylo možné importovat. Zvolený soubor je zřejmě neplatný."

#: mail.SEND_FAILURE
msgid "Could not send message: {0}"
msgstr "Nebylo možné odeslat zprávu: {0}"

#: mail.SEND_ABORTED_ADDRESS_FAILURE
msgid "Could not send message due to invalid address(es)"
msgstr "Nebylo možné odeslat zprávu kvůli neplatným adresám."

#: mail.TOO_MANY_TAGS
msgid "A tag could not be created because you have exceeded the limit."
msgstr "Štítek nebylo možné vytvořit, protože jste překročili limit."

#: offline.ONLINE_ONLY_OP
msgid ""
"You must be online to use this feature. Please check your network connection "
"and try again."
msgstr ""

#: remote.TIMEOUT
msgid "A timeout error has occurred."
msgstr ""

#: service.PARSE_ERROR
msgid "A parsing error has occurred."
msgstr "Vyskytla se chyba při syntaktické analýze."

#: service.PERM_DENIED
msgid "Permission denied."
msgstr "Přístup byl zamítnut."

#: service.FAILURE
msgctxt "service.FAILURE"
msgid "A network service error has occurred."
msgstr "Vyskytla se síťová chyba."

#: service.INVALID_REQUEST
msgid "An invalid request was made."
msgstr "Byl uskutečněn neplatný požadavek."

#: service.UNKNOWN_DOCUMENT
msgid "Server error (unknown command sent to server)."
msgstr "Chyba serveru (serveru byl poslán neznámy příkaz)."

#: service.RESOURCE_UNREACHABLE
msgid "Resource unreachable."
msgstr "Zdroj je nedostupný."

#: service.TEMPORARILY_UNAVAILABLE
msgid "Service temporarily unavailable."
msgstr "Služba je dočasně nedostupná."

#: service.AUTH_REQUIRED
msgid "Authentication Required"
msgstr "Vyžaduje se ověření"

#: service.AUTH_EXPIRED
msgid "Your session has expired. Please login again."
msgstr "Vaše relace vypršela. Prosím, přihlaste se znovu."

#: service.WRONG_HOST
msgid "Request was sent to the wrong server."
msgstr "Požadavek byl odeslaný na nesprávný server."

#: service.NON_READONLY_OPERATION_DENIED
msgid "The server is in read-only mode."
msgstr "Server je v režimu jen pro čtení."

#: service.PROXY_ERROR
msgid "Proxy error."
msgstr "Chyba proxy."

#: service.TOO_MANY_HOPS
msgid "Too many hops between servers."
msgstr "Příliš dlouhá cesta mezi servery (too many hops)."

#: service.ALREADY_IN_PROGRESS
msgid "Operation already in progress."
msgstr "Operace už probíhá."

#: service.NOT_IN_PROGRESS
msgid "Operation not in progress."
msgstr "Operace neprobíhá."

#: service.INTERRUPTED
msgid "The operation has been interrupted"
msgstr "Operace byla přerušena."

#: service.NO_SPELL_CHECK_URL
msgid "No spell check server defined."
msgstr "Nebyl definován server pro kontrolu pravopisu."

#: voice.UNABLE_TO_RETRIEVE_PROFILE_SUMMARY
msgid ""
"The system is currently unable to complete your request. We apologize for "
"this inconvenience."
msgstr ""
"Systém nebyl schopen dokončit váš požadavek. Omlouváme se za způsobené "
"nepříjemnosti."

#: voice.UNABLE_TO_RETRIEVE_CALLLOGS
msgid ""
"We are currently unable to display call records. We apologize for this "
"inconvenience."
msgstr ""
"Momentálně nedokážeme zobrazit záznamy volání. Omlouváme se za způsobené "
"nepříjemnosti. "

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_FOLDER
msgid "We are temporarily unable to display voice mail records."
msgstr "Dočasně nedokážeme zobrazovat záznamy hlasové pošty. "

#: voice.UNABLE_TO_UPDATE_CALL_FEATURES
msgctxt "voice.UNABLE_TO_UPDATE_CALL_FEATURES"
msgid ""
"We are temporarily unable to forward calls. We apologize for this "
"inconvenience."
msgstr ""
"Dočasně nedokážeme přesměrovávat volání. Omlouváme se za způsobené "
"nepříjemnosti."

#: voice.UNABLE_TO_MODIFY_CALL_FEATURES
msgctxt "voice.UNABLE_TO_MODIFY_CALL_FEATURES"
msgid ""
"We are temporarily unable to forward calls. We apologize for this "
"inconvenience."
msgstr ""
"Dočasně nedokážeme přesměrovávat volání. Omlouváme se za způsobené "
"nepříjemnosti."

#: voice.UNABLE_TO_MODIFY_VOICEMAIL_PREFS
msgid ""
"An error occurred while attempting to update your voice mail preferences. "
"Please try again."
msgstr ""
"Vyskytla se chyba při pokusu o aktualizaci nastavení vaší hlasové pošty. "
"Prosím, zkuste to znovu. "

#: voice.UNABLE_TO_FORWARD_VOICEMAIL
msgid "Your email could not be forwarded at this time. Please try again later."
msgstr ""
"Vaši poštu momentálně nebylo možné přesměrovat. Prosím, zkuste to znovu. "

#: voice.UNABLE_TO_RETRIEVE_CALL_FEATURES
msgid "Unable to retrieve call features."
msgstr "Nebylo možné zjistit možnosti volání."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_CONTENT
msgid "Unable to retrieve voice mail content."
msgstr "Nebylo možné získat obsah hlasové pošty."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_PREFS
msgid "Unable to retrieve voice mail preferences."
msgstr "Nebylo možné získat nastavení hlasové pošty."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_SUMMARY
msgid "Unable to retrieve voice mail summary."
msgstr "Nebylo možné získat shrnutí hlasové pošty."

#: voice.UNABLE_TO_MODIFY_VOICEMAIL_STATUS
msgid "Unable to modify voice mail status. "
msgstr "Nebylo možné změnit stav hlasové pošty."

#: voice.UNABLE_TO_EMPTY_VOICEMAIL_TRASH
msgid "Unable to empty voice mail trash."
msgstr "Nebylo možné vyprázdnit Koš hlasové pošty."

#: voice.UNABLE_TO_DELETE_VOICEMAIL
msgid "Unable to delete voice mail."
msgstr "Nebylo možné smazat hlasovou poštu."

#: voice.UNABLE_TO_UNDELETE_VOICEMAIL
msgid "Unable to undelete voice mail."
msgstr "Nebylo možné obnovit smazanou hlasovou poštu."

#: zclient.CLIENT_ERROR
msgid "A network service error has occurred (zclient)."
msgstr "Vyskytla se síťová chyba (zclient)."

#: zclient.IO_ERROR
msgid "A network service error has occurred (zclient/io)."
msgstr "Vyskytla se síťová chyba (zclient/io)."

#: ztaglib.TAG_EXCEPTION
msgid "A client error occurred. Please try again."
msgstr "Vyskytla se chyba klienta. Prosím, zkuste to znovu."

#: unknownError
msgid "An unknown error ({0}) has occurred."
msgstr ""
